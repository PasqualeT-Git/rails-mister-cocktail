<h2>List of doses for <%= @cocktail.name %></h2>

<ul>
  <% @cocktail.doses.each do |dose| %>
    <li><%= dose.description %> of <%= dose.ingredient.name %></li>
    <%= link_to "delete", dose_path(dose), method: :delete %>
  <% end %>
</ul>

<%= link_to "add dose", new_cocktail_dose_path(@cocktail)  %>
